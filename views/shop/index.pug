extends ../includes/navigation.pug

block content

        main
            if prods.length > 0
                .grid
                    each product in prods
                        article.card.product-item
                            header.card__header
                                h1.product__title #{product.title}
                            div.card__image
                                img(src="data:image/jpeg;base64," alt="", srcset="") 
                            div.car__content
                                h2.product__price $19.99  
                                p.product__description A very interesting book about so many even more
                            .card__content
                                button.btn Add to cart 
            else
                h1 No Products                       